                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/line_dash_horizontal"
                        app:layout_constraintBottom_toBottomOf="@+id/bcA"
                        app:layout_constraintEnd_toEndOf="@+id/bcA"
                        app:layout_constraintStart_toStartOf="@+id/bcA"
                        app:layout_constraintTop_toTopOf="@+id/bcA"
                        app:layout_constraintVertical_bias="0.25" />

                    <ImageView
                        android:id="@+id/imageView34"
                        android:layout_width="0dp"
                        android:layout_height="10dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/line_dash_horizontal"
                        app:layout_constraintBottom_toBottomOf="@+id/bcA"
                        app:layout_constraintEnd_toEndOf="@+id/bcA"
                        app:layout_constraintStart_toStartOf="@+id/bcA"
                        app:layout_constraintTop_toTopOf="@+id/bcA"
                        app:layout_constraintVertical_bias="0.6" />

                    <LinearLayout
