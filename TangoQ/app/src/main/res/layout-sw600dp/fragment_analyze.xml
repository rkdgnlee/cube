                                android:layout_height="wrap_content"
                                android:layout_marginStart="4dp"
                                android:textColor="@color/subColor800"
                                android:textSize="16dp"
                                app:layout_constraintBottom_toBottomOf="@+id/ivAExerciseStage"
                                app:layout_constraintStart_toEndOf="@+id/ivAExerciseStage"
                                app:layout_constraintTop_toTopOf="@+id/ivAExerciseStage" />

