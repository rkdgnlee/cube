                        <ImageView
                            android:id="@+id/ivA2"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_2_disabled"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/ivA3"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_3_disabled"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/ivA4"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_4_disabled"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/ivA5"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_5_disabled"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/ivA6"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_6_disabled"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/ivA7"
                            android:layout_width="match_parent"
                            android:layout_height="24dp"
                            android:layout_weight="1"
                            android:src="@drawable/icon_week_7_disabled"
                            android:visibility="visible" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout17"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/llAWeek"
                        app:layout_constraintStart_toStartOf="@+id/llAWeek"
                        app:layout_constraintTop_toTopOf="parent">

                        <View
                            android:id="@+id/vA1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
